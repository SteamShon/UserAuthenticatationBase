<%= form_tag("/awards", method: :get, class: "well form-inline") do %>
	<%= hidden_field_tag "user_class", @user_class.id %>
	From
	<%= text_field_tag "start_date", @start_date, class: "datepicker input-small" %>
	To
	<%= text_field_tag "end_date", @end_date, class: "datepicker input-small" %>
	<%= submit_tag "search" %>
<% end %>