$('#behaviorModal').on('shown', function(e) {
    var modal = $(this);
    modal.css('margin-top', (modal.outerHeight() / 2) * -1).css('margin-left', (modal.outerWidth() / 2) * -1);
    return this;
});
$("#behaviorModal .modal-body").html("<%= escape_javascript(render 'form_multiple_detail') %>");
$("#behaviorModal").modal();